
@{
    ViewBag.Title = "SignIn";
    Layout = "~/Views/Shared/_MyBlogBigLayout.cshtml";
}

<section class="post">
    <div class="row">
        <div class="col-12">
            <h2>Sign In</h2>
        </div>
        <div class="col-12">

            <ul class="actions fit">
                <li><a onclick="authManager.SignIn()" class="button primary fit">Google</a></li>
            </ul>
        </div>
    </div>
</section>
<script>
    function DocumentReadyFunc()
    {
        PrintLogMessage("SignIn", "DocumentReadyFunc", "init", LOG_LEVEL_INFO)
        tabMenuManager.ActiveSecondMenu()
        tabMenuManager.ChangeSecondMenu("Sign in", "/Blog/SignIn")
    }
</script>